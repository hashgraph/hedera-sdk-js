{
    "name": "@hashgraph/proto",
    "version": "1.0.4",
    "description": "Protobufs for the Hederaâ„¢ Hashgraph SDK",
    "main": "lib/index.js",
    "browser": "src/index.js",
    "types": "lib/index.d.ts",
    "license": "Apache-2.0",
    "author": "Launchbadge <contact@launchbadge.com>",
    "repository": "https://github.com/hashgraph/hedera-sdk-js",
    "bugs": "https://github.com/hashgraph/hedera-sdk-js/issues",
    "keywords": [
        "hedera",
        "hashgraph",
        "sdk",
        "transactions"
    ],
    "files": [
        "lib/",
        "src/"
    ],
    "engines": {
        "node": ">=10.0.0"
    },
    "browserslist": [
        "last 1 Chrome versions",
        "last 1 Firefox versions"
    ],
    "scripts": {
        "docs": "typedoc --theme minimal --mode modules --excludeNotExported --excludePrivate --excludeProtected --out docs --listInvalidSymbolLinks",
        "cp:ts": "copyfiles -u 1 src/index.d.ts src/proto.d.ts lib/",
        "compile": "run-s compile:proto compile:proto_ts compile:js",
        "compile:js": "babel src -d lib",
        "compile:proto": "pbjs -t static-module -w es6 --no-beautify --no-convert --no-delimited --no-verify -o src/proto.js src/proto/*.proto",
        "compile:proto_ts": "pbts -o src/proto.d.ts src/proto.js",
        "lint": "run-s lint:*",
        "lint:types": "tsc",
        "lint:format": "prettier src '*.json' 'src/*.js' --check",
        "lint:js": "eslint --fix 'src/**/*.js'",
        "format": "prettier src '*.json' 'src/*.js' --write",
        "build": "run-s lint compile cp:ts",
        "patch-package": "patch-package",
        "prepare": "run-s patch-package compile cp:ts"
    },
    "dependencies": {
        "protobufjs": "^6.10.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-modules-commonjs": "^7.10.4",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-preset-minify": "^0.5.1",
        "chalk": "^4.1.0",
        "copyfiles": "^2.4.0",
        "escodegen": "^1.13.0",
        "eslint": "^7.10.0",
        "estraverse": "^5.1.0",
        "jsdoc": "^3.6.6",
        "npm-run-all": "^4.1.5",
        "patch-package": "^6.2.2",
        "prettier": "^2.1.2",
        "semver": "^7.3.2",
        "tmp": "^0.2.1",
        "typescript": "^4.0.3"
    }
}
