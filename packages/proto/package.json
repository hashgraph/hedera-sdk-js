{
    "name": "@hashgraph/proto",
    "version": "2.3.0",
    "description": "Protobufs for the Hederaâ„¢ Hashgraph SDK",
    "main": "lib/index.js",
    "browser": "src/index.js",
    "types": "lib/index.d.ts",
    "license": "Apache-2.0",
    "author": "Launchbadge <contact@launchbadge.com>",
    "repository": "https://github.com/hashgraph/hedera-sdk-js",
    "bugs": "https://github.com/hashgraph/hedera-sdk-js/issues",
    "keywords": [
        "hedera",
        "hashgraph",
        "sdk",
        "transactions"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./lib/index.js",
            "require": "./lib/index.js"
        }
    },
    "files": [
        "lib/",
        "src/"
    ],
    "engines": {
        "node": ">=10.0.0"
    },
    "browserslist": [
        "last 1 Chrome versions",
        "last 1 Firefox versions"
    ],
    "scripts": {
        "docs": "typedoc --theme minimal --mode modules --excludeNotExported --excludePrivate --excludeProtected --out docs --listInvalidSymbolLinks",
        "cp:ts": "copyfiles -u 1 src/index.d.ts src/proto.d.ts lib/",
        "compile": "run-s compile:proto compile:js",
        "compile:js": "babel src -d lib",
        "compile:proto": "./generate_protos.sh",
        "lint": "run-s lint:*",
        "lint:types": "tsc",
        "lint:format": "prettier \"*.json\" \"src/*.js\" \"src/*.ts\" --check",
        "lint:js": "eslint --fix \"src/**/*.js\"",
        "format": "prettier \"*.json\" \"src/*.js\" \"src/*.ts\" --write",
        "build": "run-s compile cp:ts lint",
        "prepare": "run-s compile cp:ts"
    },
    "dependencies": {
        "long": "^4.0.0",
        "protobufjs": "^6.11.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.8",
        "@babel/core": "^7.16.12",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-modules-commonjs": "^7.16.8",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-preset-minify": "^0.5.1",
        "chalk": "^5.0.0",
        "copyfiles": "^2.4.1",
        "escodegen": "^2.0.0",
        "eslint": "^8.7.0",
        "eslint-plugin-compat": "^4.0.1",
        "eslint-plugin-deprecation": "^1.3.2",
        "eslint-plugin-ie11": "^1.0.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^37.6.3",
        "eslint-plugin-node": "^11.1.0",
        "estraverse": "^5.3.0",
        "jsdoc": "^3.6.10",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "replace-in-file": "^6.3.2",
        "semver": "^7.3.5",
        "tmp": "^0.2.1",
        "typescript": "^4.5.5"
    }
}
